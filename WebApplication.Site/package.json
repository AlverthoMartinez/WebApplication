{
  "name": "aspnetcore-vue",
  "description": "ASP.NET Core & VueJS",
  "license": "MIT",
  "scripts": {
    "start": "cross-env ASPNETCORE_ENVIRONMENT=Production NODE_ENV=production TERM=xterm dotnet run",
    "dev": "cross-env ASPNETCORE_ENVIRONMENT=Development NODE_ENV=development dotnet run",
    "build:last": "npm run build-vendor:prod && npm run build:prod",
    "build": "node build/build.js",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "build-vendor:prod": "cross-env NODE_ENV=production webpack --config webpack.config.vendor.js --progress",
    "build-vendor:dev": "cross-env NODE_ENV=development && webpack --config webpack.config.vendor.js",
    "lint": "eslint -c ./.eslintrc.js ClientApp/**/*.js  ClientApp/**/*.vue  ClientApp/**/*.json webpack*.js",
    "install": "npm run build-vendor:dev"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "core-js": "^2.5.3",
    "decamelize": "^2.0.0",
    "node-noop": "^1.0.0",
    "node-sass": "^4.9.2",
    "vue": "^2.5.16",
    "vue-router": "^2.8.1",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "vuex": "^2.5.0",
    "vuex-router-sync": "^4.3.2",
    "webpack-merge": "^4.1.3"
  },
  "devDependencies": {
    "aspnet-webpack": "^2.0.3",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.4.1",
    "clone": "^2.1.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^3.2.4",
    "css-color-names": "0.0.4",
    "css-loader": "^0.26.4",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "event-source-polyfill": "0.0.7",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "optimize-css-assets-webpack-plugin": "^1.3.2",
    "ora": "^3.0.0",
    "popper.js": "^1.14.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^4.1.1",
    "shelljs": "^0.8.2",
    "style-loader": "^0.13.2",
    "sw-precache-webpack-plugin": "^0.11.5",
    "uglify-es": "^3.3.9",
    "url-loader": "^0.5.9",
    "vue-cli": "^2.9.6",
    "vue-loader": "^14.2.2",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-hot-middleware": "^2.21.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
